{
  "name": "phishiq-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "nest start --watch",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "generate:master": "prisma generate --schema=prisma/master/schema/schema.prisma",
    "generate:tenant": "prisma generate --schema=prisma/tenant/schema",
    "generate:all": "npm run generate:master && npm run generate:tenant",
    "migrate:master": "npx prisma migrate dev --schema=prisma/master/schema/schema.prisma",
    "migrate:tenant": "npx prisma migrate dev --schema=prisma/tenant/schema",
    "migrate:all": "npm run migrate:master && npm run migrate:tenant",
    "studio:master": "npx prisma studio --schema=prisma/master/schema/schema.prisma",
    "studio:tenant": "npx prisma studio --schema=prisma/tenant/schema",
    "studio:all": "npm run studio:master && npm run studio:tenant",
    "deploy:master": "npx prisma migrate deploy --schema=prisma/master/schema/schema.prisma",
    "deploy:tenant": "npx prisma migrate deploy --schema=prisma/tenant/schema",
    "deploy:all": "npm run studio:master && npm run studio:tenant",
    "push:master": "npx prisma db push --schema=prisma/master/schema/schema.prisma",
    "push:tenant": "npx prisma db push --schema=prisma/tenant/schema",
    "push:all": "npm run push:master && npm run push:tenant",
    "format:master": "npx prisma format --schema=prisma/master/schema/schema.prisma",
    "format:tenant": "npx prisma format --schema=prisma/tenant/schema",
    "format:all": "npm run format:master && npm run format:tenant",
    "reset:master": "npx prisma migrate reset --schema=prisma/master/schema/schema.prisma",
    "reset:tenant": "npx prisma migrate reset --schema=prisma/tenant/schema",
    "reset:all": "npm run reset:master && npm run reset:tenant"
  },
  "dependencies": {
    "@keycloak/keycloak-admin-client": "^26.2.4",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-socket.io": "^11.0.12",
    "@nestjs/websockets": "^11.0.12",
    "@prisma/client": "^6.5.0",
    "bcrypt": "^5.1.1",
    "jwks-rsa": "^3.2.0",
    "keycloak-admin": "^1.14.22",
    "nestjs-zod": "^4.3.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@rocketseat/eslint-config": "^2.2.2",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "prisma": "^6.5.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
