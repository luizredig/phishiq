model Departamento {
  id     String              @id @default(uuid())
  nome   String
  testes TesteDepartamento[]
  logs   Log[]

  ativo         Boolean   @default(true)
  criadoEm      DateTime  @default(now())
  criadoPor     String?
  atualizadoEm  DateTime  @updatedAt
  atualizadoPor String?
  inativadoEm   DateTime?
  inativadoPor  String?
}
