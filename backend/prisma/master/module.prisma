model Module {
  id             String    @id @default(uuid())
  name           String
  slug           String    @unique
  is_active      Boolean   @default(true)
  created_by     String
  created_at     DateTime  @default(now())
  updated_by     String
  updated_at     DateTime  @updatedAt
  inactivated_at DateTime?
  inactivated_by String?

  tenant_modules TenantModule[]
}
