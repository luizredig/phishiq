model TipoDeTeste {
  id        String     @id @default(uuid())
  nome      String
  campanhas Campanha[]
  logs      Log[]

  ativo       Boolean   @default(true)
  criadoEm    DateTime  @default(now())
  criadoPor   String?
  editadoEm   DateTime  @updatedAt
  editadoPor  String?
  deletadoEm  DateTime?
  deletadoPor String?
}
