model Departamento {
  id     String              @id @default(uuid())
  nome   String
  testes TesteDepartamento[]
  logs   Log[]

  ativo       Boolean   @default(true)
  criadoEm    DateTime  @default(now())
  criadoPor   String?
  editadoEm   DateTime  @updatedAt
  editadoPor  String?
  deletadoEm  DateTime?
  deletadoPor String?
}
